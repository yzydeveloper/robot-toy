{
    "name": "@robot-toy/pusher",
    "builds": [
        {
            "src": "dist/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "x-vercel-ip-timezone",
                    "value": "Asia/Shanghai"
                }
            ]
        }
    ]
}
